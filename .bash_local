#
# ~/.bash_aaron
# My bash envirronment
#

# source bash ps1 file
if [ -f ~/.bash_ps1 ];then
    source ~/.bash_ps1
fi

# aliases
alias ll='ls -la'
alias llt='ls -lAht'

# source autojump
source /usr/share/autojump/autojump.bash

# dotfile git bare repository
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
